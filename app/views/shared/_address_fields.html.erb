<div class="nested-fields">
  <%= link_to_remove_association '<i class="fa fa-times"></i>'.html_safe, f, title: 'Remove',
                                  class:'btn btn-danger', style:'float:right;' %>
  <hr>
  <div class="form-group">
    <%= f.label :street, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
      <%= f.email_field :street, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :city, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
      <%= f.email_field :city, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :state, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
      <%= f.email_field :state, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :zipcode, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
      <%= f.email_field :zipcode, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :phone_number, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
      <%= f.email_field :phone_number, class: 'form-control' %>
    </div>
  </div>
</div>
